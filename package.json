  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "atomTestRunner": "atom-test-runner-jest",
  "activationCommands": {
    "atom-workspace": [
      "atom-beautify:help-debug-editor",
      "atom-beautify:beautify-editor",
      "atom-beautify:open-settings",
      "core:save",
      "core:save-as"
    ],
    ".tree-view .file .name": [
      "atom-beautify:beautify-file"
    ],
    ".tree-view .directory .name": [
      "atom-beautify:beautify-directory"
    ]
  },
  "keywords": [
    "atom",
    "unibeautify",
    "beautify",
    "beautifier",
    "format",
    "pretty",
    "clean"
  ],
  "dependencies": {
    "@unibeautify/beautifier-eslint": "^0.5.0",
    "@unibeautify/beautifier-js-beautify": "^0.3.2",
    "@unibeautify/beautifier-php-cs-fixer": "^0.3.0",
    "@unibeautify/beautifier-prettier": "^0.8.0",
    "@unibeautify/beautifier-prettydiff": "^0.5.3",
    "@unibeautify/beautifier-yapf": "^0.1.0",
    "cosmiconfig": "^5.0.4",
    "eslint": "^4.19.1",
    "js-beautify": "^1.7.5",
    "prettier": "^1.12.1",
    "prettydiff2": "^2.2.7",
    "triple-beam": "^1.2.0",
    "unibeautify": "^0.15.0",
    "winston": "^3.0.0-rc5"
  },
  "devDependencies": {
    "@types/atom": "^1.26.0",
    "@types/cosmiconfig": "^5.0.1",
    "@types/event-kit": "^2.4.0",
    "@types/jest": "^22.2.3",
    "atom-test-runner-jest": "^0.1.3",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.1.2",
    "jest": "^22.4.4",
    "jest-preset-atom": "^0.2.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-clean-code": "^0.2.7",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile && npm run docs",
    "clean": "rimraf dist",
    "compile": "tsc",
    "docs": "ts-node script/index",
    "lint": "tslint '{src,spec,script}/**/*.ts'",
    "prepublishOnly": "npm run build",
    "start": "tsc --watch",
    "test": "atom --test spec"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ]
}
