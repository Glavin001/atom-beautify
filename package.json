  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "atomTestRunner": "atom-test-runner-jest",
  "activationCommands": {
    "atom-workspace": [
      "atom-beautify:help-debug-editor",
      "atom-beautify:beautify-editor",
      "atom-beautify:open-settings",
      "core:save",
      "core:save-as"
    ],
    ".tree-view .file .name": [
      "atom-beautify:beautify-file"
    ],
    ".tree-view .directory .name": [
      "atom-beautify:beautify-directory"
    ]
  },
  "keywords": [
    "atom",
    "unibeautify",
    "beautify",
    "beautifier",
    "format",
    "pretty",
    "clean"
  ],
  "dependencies": {
    "@unibeautify/beautifier-black": "^0.1.1",
    "@unibeautify/beautifier-clang-format": "^0.2.0",
    "@unibeautify/beautifier-csscomb": "^0.1.0",
    "@unibeautify/beautifier-eslint": "^0.6.0",
    "@unibeautify/beautifier-gofmt": "^0.1.0",
    "@unibeautify/beautifier-js-beautify": "^0.4.0",
    "@unibeautify/beautifier-php-codesniffer": "^0.1.1",
    "@unibeautify/beautifier-php-cs-fixer": "^0.4.0",
    "@unibeautify/beautifier-prettier": "^0.9.0",
    "@unibeautify/beautifier-prettydiff": "^0.6.0",
    "@unibeautify/beautifier-sqlformat": "0.0.1",
    "@unibeautify/beautifier-tslint": "^0.1.0",
    "@unibeautify/beautifier-yapf": "^0.3.0",
    "cosmiconfig": "^5.0.7",
    "csscomb": "^4.2.0",
    "eslint": "^5.9.0",
    "js-beautify": "^1.8.8",
    "prettier": "^1.15.2",
    "prettydiff2": "^2.2.7",
    "triple-beam": "^1.3.0",
    "tslint": "5.11.0",
    "unibeautify": "^0.17.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/atom": "1.31.0",
    "@types/cosmiconfig": "5.0.3",
    "@types/event-kit": "2.4.0",
    "@types/jest": "23.3.9",
    "atom-test-runner-jest": "0.1.3",
    "@types/lodash": "4.14.118",
    "@types/node": "10.12.9",
    "jest": "23.6.0",
    "jest-preset-atom": "0.2.3",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint-clean-code": "0.2.9",
    "tslint-microsoft-contrib": "5.2.1",
    "typescript": "3.1.6"
  },
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile && npm run docs",
    "clean": "rimraf dist",
    "compile": "tsc",
    "docs": "ts-node script/index",
    "lint": "tslint '{src,spec,script}/**/*.ts'",
    "prepublishOnly": "npm run build",
    "start": "tsc --watch",
    "test": "atom --test spec"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "code-format.file": {
      "versions": {
        "0.1.0": "provideFileCodeFormat"
      }
    }
  }
}
